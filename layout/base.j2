<!DOCTYPE html>
<html lang="en">
  <head>
    {% block starthead %} {% endblock starthead %}
    <meta charset="{{ resource.meta.charset }}">
    <title>{% block title %}{{ resource.meta.title }}{% endblock %} | {{ site_title }}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ resource.meta.description }}">
    <meta name="author" content="{{ resource.meta.author }}">

    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />

    {# Le HTML5 shim, for IE6-8 support of HTML elements #}
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    {% block basecss %}
    <link href="{{ media_url('css/bootstrap.css') }}" rel="stylesheet">
    <link href="{{ media_url('css/offcanvas.css') }}" rel="stylesheet">
    <link href="{{ media_url('css/style.css') }}" rel="stylesheet">

    <style type="text/css">
        body {
            padding-top: 60px;
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of
            floated navbar
            text */
            .navbar-text.pull-right
            {
                float:
                none;
                padding-left:
                5px;
                padding-right:
                5px;
            }
        }
    </style>

    {% endblock basecss %}
    {% block css %}{% endblock %}



    {% block favicons %}
    {# Place favicon.ico & apple-touch-icon.png in the root of your domain and delete these references #}
    <link rel="apple-touch-icon" href="{{ media_url('img/apple-touch-icon.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ media_url('img/appleimgim-touch-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ media_url('img/apple-touch-icon-114x114.png') }}">
    {% endblock favicons %}

    {% block headjs %} {% endblock headjs %}

    {% block endhead %} {% endblock endhead %}
    
    {% block analytics %} {% include "analytics.j2" %} {% endblock analytics %}
  </head>

  <body class="{{ resource.meta.id if resource.meta.id else resource.slug }}">

    {% block body %}
    {% block jumbotron %} {% endblock jumbotron %}
    <div class="container" id="top container-main">
      {% block container %}

      {% endblock container %}
      <hr>
      <footer>
        {% block footer %}
            <p> &copy 2014 <a href="{{ author.url }}">{{ author.name }}</a>. Powered by <a href="http://hyde.github.io">Hyde</a>.</p>
            <p><a href="http://github.com/xLegoz"><img src="{{ media_url('img/github.png') }}" /></a>
            <a href="http://ca.linkedin.com/in/joeypereira"><img src="{{ media_url('img/linkedin.png') }}" /></a></p>
        {% endblock footer %}
      </footer>
    </div> <!-- /container -->

    {% endblock body %}

    {% block js %}

    {% block jquery %}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ media_url('js/libs/jquery-1.10.1.min.js') }}">\x3C/script>')</script>
    {% endblock jquery %}

    {% block bootstrap_scripts %}
    <script src="{{ media_url('js/libs/bootstrap.min.js') }}"></script>
    {# commented out plugins of bootstrap #}
    {#    <script src="{{ media_url('js/libs/affix.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/alert.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/button.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/carousel.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/collapse.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/dropdown.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/modal.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/popover.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/scrollspy.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/tab.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/tooltip.js') }}"></script>#}
    {#    <script src="{{ media_url('js/libs/transition.js') }}"></script>#}
    <script src="{{ media_url('js/libs/offcanvas.js') }}"></script>
    {% endblock bootstrap_scripts %}

    {% block scripts %}
    <script src="{{ media_url('js/scripts.js') }}"></script>
    {% endblock scripts %}


    <!--[if lt IE 7 ]>
      <script src="{{ media_url('js/libs/dd_belatedpng.js') }}"></script>
      <script>DD_belatedPNG.fix('img, .png_bg'); // Fix any <img> or .png_bg bg-images. Also, please read goo.gl/mZiyb </script>
    <![endif]-->

    {% endblock js %}
  </body>
</html>
